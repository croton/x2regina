/* Tag profile for generating HTML tags */
extension = HTML

expand_keyword = "?\1"
expand_replace = '<\1\c>'
expand_replace = '</\1>'

expand_keyword = "@\1"
expand_replace = '<\1>\c</\1>'

expand_keyword = "#\1"
expand_replace = '<\1 class="\c"></\1>'

expand_keyword = $\1
expand_replace = '<\1 \c/>'

expand_keyword = "\1 ?>"
expand_replace = '\1 \c="">'

expand_keyword = "\1 c>"
expand_replace = '\1 class="\c">'

